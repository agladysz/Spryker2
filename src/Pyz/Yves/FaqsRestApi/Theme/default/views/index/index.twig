{% extends template('page-layout-main') %}

{% define data = {
    faqs: _view.faqs
} %}

{% block content %}
    <div>
        {% for row in data.faqs %}
            <div>
                <h3> {{ row.question }} </h3>
                <p> {{ row.answer }} </p>
                <h4> {{ row.votes }} </h4>
            </div>
        {% endfor %}
    </div>
{% endblock %}
